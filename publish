(cd src && raptor-optimizer profile=production publish=true) && raptor-jsdoc src=node_modules/raptor/lib/raptor/raptor_modules/ output-dir=api profile=production include-filename=false publish=true props=node_modules/raptor/package.json,node_modules/raptor/jsdoc-props.json clean=api && git add --all '**' && git commit -m "Published documentation" && git push origin master