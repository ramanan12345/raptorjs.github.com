<c:template
    xmlns:c="core"
    xmlns:w="widgets"
    name="components/nav/Nav"
    params="rootAttrs,navItems">
    
    <ul w:widget="components.nav.Nav.NavWidget" c:attrs="rootAttrs">
        <c:for each="navItem in navItems">
            <c:invoke function="navItem.render()"/>
        </c:for>
    </ul>
</c:template>