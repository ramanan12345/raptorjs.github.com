<c:template
    xmlns:c="core"
    xmlns:w="widgets"
    xmlns:docs="docs"
    xmlns:main="main"
    params="includes,samples">
    
    <docs:urls basePath=".."/>
    
    <![CDATA[<!doctype html>]]>
    
    <html>
        <head>
            <title>RaptorJS: Raptor Templates</title>
            <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
            ${includes.head}
            <docs:google-analytics/>
        </head>
        <body data-spy="scroll">
            <main:top-nav activeItem="raptor-templates"/>
            <div class="content">
                <c:include template="content/raptor-templates"/>
            </div>
            
            
            ${includes.body}
            
            <w:initWidgets/>
        </body>
    </html>
</c:template>

